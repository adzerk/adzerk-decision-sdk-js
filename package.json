{
  "name": "@adzerk/decision-sdk",
  "version": "1.0.0-beta.6",
  "description": "SDK for interacting with Adzerk's Decision API",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'tsc -p' .",
    "build": "tsc -p ./src && sed -i \"s/{NPM_PACKAGE_VERSION}/$npm_package_version/g\" lib/client.js && parcel build lib/index.js --out-dir dist --out-file adzerk-decision-sdk.js --global AdzerkDecisionSdk",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adzerk/adzerk-decision-sdk-js.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adzerk/adzerk-decision-sdk-js/issues"
  },
  "homepage": "https://github.com/adzerk/adzerk-decision-sdk-js#readme",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.3",
    "husky": "^5.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "node-fetch": "^2.6.0",
    "node-forge": ">=0.10.0",
    "node-notifier": ">=8.0.1",
    "nodemon": "^1.19.4",
    "npm-force-resolutions": "0.0.3",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "size-limit": "^4.9.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "debug": "^4.1.1",
    "form-data": "^2.5.1",
    "isomorphic-unfetch": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "lib/index.js"
    }
  ]
}
