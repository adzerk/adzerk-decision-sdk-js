  "bugs": {
    "url": "https://github.com/adzerk/adzerk-decision-sdk-js/issues"
  },
  "homepage": "https://github.com/adzerk/adzerk-decision-sdk-js#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
